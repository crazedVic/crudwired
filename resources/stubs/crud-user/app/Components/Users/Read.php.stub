<?php

namespace App\Components\Users;

use App\Models\User;
use Crazed\Crudwired\Components\ModalComponent;

class Read extends ModalComponent
{
    public $user;

    public function render()
    {
        return view('users.read');
    }

    public function show(User $user)
    {
        $this->user = $user;

        $this->toggle();
    }
}
